  "executable": "VL-Launcher",
  "splashscreen": {
    "imageFile": ""
  },
  "name": "VL-Launcher",
  "author": "",
  "singleInstance": false,
  "environment": "Production",
  "build": {
    "appId": "launcher.vl.cstu.xyz",
    "productName": "VL-Launcher",
    "copyright": "Copyright Â© 2021 TURX",
    "buildVersion": "0.0.4",
    "compression": "maximum",
